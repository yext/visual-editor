{
  "name": "@yext/visual-editor",
  "description": "Component library for Yext Pages Visual Editor",
  "version": "1.1.0-alpha.5",
  "author": "sumo@yext.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yext/visual-editor.git"
  },
  "sideEffects": [
    "./dist/style.css"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "dist/visual-editor.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/visual-editor.js",
      "default": "./dist/visual-editor.js"
    },
    "./plugin": {
      "import": "./dist/plugin/plugin.js",
      "default": "./dist/plugin/plugin.js"
    },
    "./style.css": "./dist/style.css",
    "./editor.css": "./dist/editor.css"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build:components": "tsc && vite build",
    "build:plugin": "vite build --config vite.config.plugin.ts",
    "build": "pnpm run build:components && pnpm run build:plugin",
    "test": "pnpm run test:editor && pnpm run test:components",
    "test:editor": "vitest run --exclude './src/components/**/*.test.tsx' --browser.enabled false",
    "test:components": "vitest run ./src/components/**",
    "test:components:local": "./scripts/localScreenshots.sh",
    "prettier": "prettier --write --cache .",
    "lint": "oxlint --fix --ignore-pattern 'src/vite-plugin/templates/*'",
    "autofix": "pnpm run lint && pnpm run prettier",
    "prepare": "pnpm build",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "generate-component-docs": "api-extractor run --local ; node generate-docs.mjs ; tsx scripts/generateComponentTypeDefinitions.ts",
    "docs:dev": "vitepress dev .",
    "docs:build": "vitepress build .",
    "docs:preview": "vitepress preview .",
    "i18n:extract:platform": "i18next-cli extract --config i18next-cli.platform.config.ts --quiet",
    "i18n:extract:components": "i18next-cli extract --config i18next-cli.components.config.ts --quiet",
    "i18n:translate:platform": "pnpm exec tsx scripts/generateTranslations.ts --type platform",
    "i18n:translate:platform:dry": "pnpm exec tsx scripts/generateTranslations.ts --type platform --dry-run",
    "i18n:propagate:components": "pnpm exec tsx scripts/propagatePlatformToComponents.ts",
    "i18n:check:interpolation": "pnpm exec tsx scripts/checkInterpolationVariables.ts",
    "i18n:lint": "i18next-cli lint --config i18next-cli.platform.config.ts --quiet && i18next-cli lint --config i18next-cli.components.config.ts --quiet && pnpm run i18n:check:interpolation",
    "i18n:update": "pnpm run i18n:extract:platform && pnpm run i18n:extract:components && pnpm run i18n:translate:platform && pnpm run i18n:propagate:components && pnpm run i18n:lint"
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.29.6",
    "@microsoft/api-extractor": "^7.57.6",
    "@microsoft/api-extractor-model": "^7.33.4",
    "@puckeditor/core": "0.21.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.3.1",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "i18next": "^25.8.13",
    "lucide-react": "^0.414.0",
    "lz-string": "1.5.0",
    "next-themes": "^0.3.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "pure-react-carousel": "^1.35.0",
    "react-collapsed": "^4.2.0",
    "react-color": "^2.19.3",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.7.4",
    "react-international-phone": "^4.8.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.1",
    "tsx": "^4.21.0"
  },
  "engines": {
    "node": "^18 || ^20.2.0 || ^22"
  },
  "packageManager": "pnpm@9.12.3",
  "lint-staged": {
    "*": "prettier --write --cache --ignore-unknown",
    "*.{ts,js,tsx,jsx}": "oxlint --fix --ignore-pattern 'src/vite-plugin/templates/*'"
  },
  "devDependencies": {
    "@capsizecss/core": "^4.1.3",
    "@capsizecss/metrics": "^3.6.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest-axe": "^3.5.9",
    "@types/mapbox-gl": "^2.7.21",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.19.34",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.28",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.3.7",
    "@types/semver": "^7.7.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "autoprefixer": "^10.4.27",
    "awesome-phonenumber": "^7.8.0",
    "axe-core": "^4.11.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.3.3",
    "generate-license-file": "^3.8.1",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "i18next-cli": "^1.46.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.5.2",
    "minimist": "^1.2.8",
    "pixelmatch": "^7.1.0",
    "playwright": "1.55.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.8.1",
    "react-icons": "^5.5.0",
    "semver": "^7.7.4",
    "tailwindcss": "^3.4.19",
    "tsup": "8.3.5",
    "typescript": "^5.9.3",
    "uuid": "11.0.3",
    "vite": "^5.4.21",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@yext/pages-components": "^2.0.0",
    "@yext/search-headless-react": "^2.7.1",
    "@yext/search-ui-react": "^2.1.0",
    "mapbox-gl": "^2.9.2",
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  }
}
