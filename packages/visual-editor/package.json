{
  "name": "@yext/visual-editor",
  "description": "Component library for Yext Pages Visual Editor",
  "version": "0.0.54",
  "type": "module",
  "author": "sumo@yext.com",
  "types": "dist/index.d.ts",
  "main": "dist/visual-editor.cjs",
  "module": "dist/visual-editor.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/visual-editor.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/visual-editor.cjs"
      }
    },
    "./plugin": {
      "default": "./dist/plugin/plugin.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build:components": "tsc && vite build",
    "build:plugin": "vite build --config vite.config.plugin.ts",
    "build": "pnpm run build:components && pnpm run build:plugin",
    "test": "pnpm run test:editor && pnpm run test:components",
    "test:editor": "vitest run --exclude './src/components/**/*.test.tsx' --browser.enabled false",
    "test:components": "vitest run ./src/components/**",
    "test:components:local": "./scripts/localScreenshots.sh",
    "prettier": "prettier --write --cache .",
    "lint": "oxlint --fix --ignore-pattern 'src/vite-plugin/templates/*'",
    "autofix": "pnpm run lint && pnpm run prettier",
    "prepare": "pnpm build",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "generate-component-docs": "api-extractor run --local ; node generate-docs.mjs",
    "docs:dev": "vitepress dev .",
    "docs:build": "vitepress build .",
    "docs:preview": "vitepress preview .",
    "generate-registry": "tsx src/components/registry/build-registry.cts",
    "build-registry-docs": "pnpm run docs:build && pnpm run generate-registry",
    "i18n:insert": "pnpm exec tsx scripts/insertI18n.ts",
    "i18n:insert:dry": "pnpm exec tsx scripts/insertI18n.ts --dry-run",
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:translate": "pnpm exec tsx scripts/generateTranslations.ts",
    "i18n:translate:dry": "pnpm exec tsx scripts/generateTranslations.ts --dry-run",
    "i18n:sort": "pnpm exec tsx scripts/sortTranslations.ts",
    "i18n:update": "pnpm run i18n:scan && pnpm run i18n:translate && pnpm run i18n:sort",
    "i18n:check-empty": "pnpm exec tsx scripts/checkEmptyTranslations.ts"
  },
  "dependencies": {
    "@measured/puck": "0.19.3",
    "@microsoft/api-documenter": "^7.26.29",
    "@microsoft/api-extractor": "^7.52.8",
    "@microsoft/api-extractor-model": "^7.30.6",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-query": "^5.71.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.2.3",
    "i18next": "^25.2.1",
    "lucide-react": "^0.414.0",
    "lz-string": "1.5.0",
    "next-themes": "^0.3.0",
    "picocolors": "^1.0.1",
    "prompts": "^2.4.2",
    "pure-react-carousel": "^1.32.0",
    "react-aria-components": "^1.11.0",
    "react-color": "^2.19.3",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.5.2",
    "react-international-phone": "^4.6.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tsx": "^4.16.3"
  },
  "engines": {
    "node": "^18 || ^20.2.0 || ^22"
  },
  "packageManager": "pnpm@9.12.3",
  "lint-staged": {
    "*": "prettier --write --cache --ignore-unknown",
    "*.{ts,js,tsx,jsx}": "oxlint --fix --ignore-pattern 'src/vite-plugin/templates/*'"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest-axe": "^3.5.9",
    "@types/mapbox-gl": "^2.7.5",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.11",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.3",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.4",
    "autoprefixer": "^10.4.2",
    "awesome-phonenumber": "^7.5.0",
    "axe-core": "^4.10.3",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "generate-license-file": "^3.5.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.4",
    "i18next-scanner": "^4.6.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.7",
    "minimist": "^1.2.8",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.54.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.3.3",
    "react-icons": "^5.2.1",
    "semver": "^7.6.3",
    "tailwindcss": "^3.4.6",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "tsup": "8.3.5",
    "typescript": "^5.5.4",
    "uuid": "11.0.3",
    "vite": "^5.3.5",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@yext/pages-components": "^1.1.9",
    "@yext/search-headless-react": "^2.5.4",
    "@yext/search-ui-react": "^1.9.3",
    "mapbox-gl": "^2.9.2",
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  }
}
